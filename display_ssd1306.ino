void showBitmap(void) {
  if (oled_connected) { display.clearDisplay();
    display.drawBitmap(0, 0, bitmap, bitmap_height, bitmap_width, WHITE);
    display.display();
  }; 
  
  if (tft_connected) {tft.drawRGBBitmap (0,0,splash_tft,240,240);
    delay(3000);
    tft.fillScreen(ST77XX_BLACK);
  };
}

void showEthconnected() {
  if (oled_connected) {display.drawBitmap(0, 0, bitmapEthOn, 16, 16, WHITE);};
  if (tft_connected) {tft.drawBitmap(25, 5, bitmapEthOn, 16, 16, GREEN);};
}

void showEthNot() {
  if (tft_connected) {tft.drawBitmap(25, 5, bitmapEthOn, 16, 16, RED);};
}

